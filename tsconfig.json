{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noStrictGenericChecks": true,
    "target": "es6",
    "types": [
      "node",
      "jasmine"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "xpx-typescript-angular-sdk": [
        "dist/xpx-typescript-angular-sdk"
      ],
      "xpx-typescript-angular-sdk/*": [
        "dist/xpx-typescript-angular-sdk/*"
      ],
      "rxjs": ["node_modules/rxjs"]
    }
  }
}
